{
    "name": "orphans/shoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Gloudemans",
            "email": "info@robgloudemans.nl"
        },
        {
            "name": "Ed Jeavons",
            "email": "eddie@edjeavons.co.uk"
        }
    ],
    "require": {
        "illuminate/support": "^6|^7|^8|^9|^10",
        "illuminate/session": "^6|^7|^8|^9|^10",
        "illuminate/events": "^6|^7|^8|^9|^10"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0|~6.0|~7.0|~8.0|9.0",
        "mockery/mockery": "~0.9.0|~1.4.4",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Gloudemans\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gloudemans\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
        "gloudemans/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Gloudemans\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Gloudemans\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
